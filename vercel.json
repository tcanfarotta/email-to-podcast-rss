{
  "buildCommand": "echo 'No build required'",
  "outputDirectory": ".",
  "framework": null,
  "functions": {
    "api/index.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/webhook/postmark",
      "destination": "/api"
    },
    {
      "source": "/rss/feed.xml",
      "destination": "/api"
    },
    {
      "source": "/rss/feed/:feedId",
      "destination": "/api"
    },
    {
      "source": "/podcasts/:file",
      "destination": "/api"
    }
  ]
}